{
  "compilerOptions": {
    "target": "es2021",
    "lib": ["ES2021"],
    "module": "Node16",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  }
}
